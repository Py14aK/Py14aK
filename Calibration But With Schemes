proc Sort data=revnew.scored_all_bkwrd_cal   nodupkey sortsize=10G;
	by descending  period month customer_id model;
run;
proc Sort data=a2;
	by descending period;
run;

data revnew.beh_a;
	do _N_=1 by 1 until(last.period);
		set A2(keep=BEH_INTERCEPT model BEH_SLOPE period ) end=eof;
		by descending period;
		BEH_INTERCEPTc=BEH_INTERCEPT;
		BEH_SLOPEc=BEH_SLOPE;	 month=period-1;
	end;
run;

proc Sort data=a1;
	by descending period;
run;

data revnew.app_a;
	do _N_=1 by 1 until(last.period);
		set A1(keep=APP_SLOPE model APP_INTERCEPT period month) end=eof;
		by descending period;
		APP_INTERCEPTc=APP_INTERCEPT;
		APP_SLOPEc=APP_SLOPE;
		 month=period-1;
	end;
run;

proc Sql;
	create table revnew.scored_all_cal
		as select a.APP_INTERCEPTc,A.MODEL, a.APP_SLOPEc,C.BEH_INTERCEPTc, C.BEH_SLOPEc,C.MODEL  ,b.* 
		from revnew.scored_all_bkwrd_cal b
		left join revnew.app_a a
		on B.period=A.month
		and b.model=a.model
		LEFT JOIN REVNEW.BEH_A C
			ON B.period=C.MONTH
		AND	B.MODEL=C.MODEL;
quit;
