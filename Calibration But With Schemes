/*cHECK MISSING*/
proc Format;
	value $missfmt ' '='Missing' other='Not Missing';
	value  missfmt  . ='Missing' other='Not Missing';
run;
/**/
/*proc Freq data=scored_all_frwd_cal_app;*/
/*	format  APP_INTERCEPTC missfmt.;*/
/*	tables  APP_INTERCEPTC / missing missprint nocum nopercent;*/
/*run;*/

proc Freq data=revnew.scored_all_frwd_cal;
	format  BEH_INTERCEPT BEH_SLOPE APP_INTERCEPT APP_SLOPE missfmt.;
	tables  BEH_INTERCEPT BEH_SLOPE APP_INTERCEPT APP_SLOPE / missing missprint nocum nopercent out=revnewold_cal_miss;
	tables  BEH_INTERCEPTc BEH_SLOPEc APP_INTERCEPTc APP_SLOPEc / missing missprint nocum nopercent;
run;
